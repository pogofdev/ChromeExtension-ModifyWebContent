{
  "name": "Get pages source",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get pages source from a popup",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "<all_urls>"],
  "content_scripts": [
    {
      "matches": ["https://world.tmall.com/item/*"],
      "js": ["getPagesSource.js"]
    }
  ]
}